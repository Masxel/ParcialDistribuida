@page "/entranceindex"

<div class="mt-3 card" style="width:55%; text-align:center; margin-left:10%">
    <div class="card-header">
        <span>
            <i class="oi oi-magnifying-glass" /> Consulta tickets
            <button class="btn btn-sm btn-primary float-end mx-2" type="submit"><i class="oi oi-check" /> Realizar busqueda</button>
        </span>
    </div>
    <div class="card-body">
        <div class="row" style="text-align:center; display:inline-block">
            <input style="width: 320px;" class="form-control" id="numberticket" type="number" placeholder="Ingrese el número del ticket" min="1"/>
        </div>
    </div>
</div>



